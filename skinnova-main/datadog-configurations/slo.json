        {
            "id": "f71c2610f6af5a8299bf18c0e6d28a28",
            "name": "LLM Hallucination Detection Error Rate SLO",
            "tags": [
                "check_type:api",
                "team:dev-team",
                "team:ai-engineering-team",
                "service:fastapi"
            ],
            "monitor_tags": [],
            "thresholds": [
                {
                    "timeframe": "7d",
                    "target": 75.0,
                    "target_display": "75.",
                    "warning": 85.0,
                    "warning_display": "85."
                }
            ],
            "type": "metric",
            "type_id": 1,
            "description": "This SLO measures the reliability of the hallucination detection pipeline by tracking the percentage of LLM interactions that are successfully processed without hallucination detection or JSON parsing failures.\n\nErrors indicate failures in safety validation or structured output parsing, which can compromise response trustworthiness and downstream enforcement logic.",
            "timeframe": "7d",
            "warning_threshold": 85.0,
            "target_threshold": 75.0,
            "query": {
                "denominator": "sum:llm.chats.total{*}.as_count()",
                "numerator": "sum:llm.chats.total{*}.as_count() - (sum:llm.errors.hallucination_detection_failure{*}.as_count() + sum:llm.errors.json_parsing_failure{*}.as_count())"
            },
            "creator": {
                "name": "skinnova",
                "handle": "contact@skinnova.beauty",
                "email": "contact@skinnova.beauty"
            },
            "created_at": 1767126485,
            "modified_at": 1767177830
        },
